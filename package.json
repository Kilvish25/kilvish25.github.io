{
  "name": "kilvish25.github.io",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && touch out/.nojekyll",
    "start": "next start",
    "lint": "next lint",
    "export": "next build",
    "clean": "rm -rf .next out",
    "rebuild": "npm run clean && npm run build",
    "deploy": "next build && touch out/.nojekyll && echo \"Personal portfolio site for Dharmendra Ahirwar (kilvish25)\" > out/README.md && git add out/ -f && git commit -m \"Deploy to GitHub Pages\" && git subtree push --prefix out origin gh-pages"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/typography": "^0.5.16",
    "framer-motion": "^12.5.0",
    "next": "14.1.3",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^9.16.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.3",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
